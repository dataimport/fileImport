<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
	default-lazy-init="true">
	
	<bean id="fileManager" class="com.xxx.admin.manager.FileManager" />
	<bean id="taskManager" class="com.xxx.admin.manager.TaskManager" />
	<bean id="txtData" class="com.xxx.admin.data.TxtData" />
	<bean id="mongoDao" class="com.xxx.admin.data.mongo.MongoDaoImpl" />
	<bean id="mongoDBRepository"
        class="com.xxx.admin.data.mongo.MongoDBRepositoryImpl">
        <property name="mongoTemplate" ref="mongoTemplate" />
    </bean>
</beans>
